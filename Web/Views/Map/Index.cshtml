@model List<GenieMapTemp>
@{
    int count = 1;
    int type = ViewBag.Type;
}
<div class="module-title">
    <Abar sid="@ViewBag.MySid" href="/Index/Index">精灵主页</Abar>&gt;图鉴
</div>
<div class="module-content">
    类型：<Acheak sid="@ViewBag.MySid" href="/Map/Index?t=1" on-cheak="1" on-value="@type">普通</Acheak>|
    <Acheak sid="@ViewBag.MySid" href="/Map/Index?t=2" on-cheak="2" on-value="@type">精英</Acheak>|
    <Acheak sid="@ViewBag.MySid" href="/Map/Index?t=3" on-cheak="3" on-value="@type">珍稀</Acheak>|
    <Acheak sid="@ViewBag.MySid" href="/Map/Index?t=4" on-cheak="4" on-value="@type">传说</Acheak>
    <div class="list">
        @foreach (var item in Model)
        {
            <div class="row">
                @Html.Raw(((ViewBag.PageIndex - 1) * ViewBag.PageSize + count).ToString() + ".")
                <img alt="." src="/res/genie/@string.Format("{0}",item.isHave?item.genie.img:"m_s.gif")" width="30" height="30" />
                <Abar sid="@ViewBag.MySid" href="/Map/Info/@item.genie.genieId">@item.genie.name</Abar>
                @string.Format("{0}", item.isHave ? "" : "(待解锁)")
            </div>
            count++;
        }
        @if (Model.Count == 0)
        {
            <span>暂无图鉴.</span>
        }
    </div>
</div>
<PageBar page-class="content" page-size="@ViewBag.PageSize" page-Total="@ViewBag.Total" page-Url="@RequestHelper.GetRequestUrl(ViewContext.HttpContext.Request)"></PageBar>

<Abar sid="@ViewBag.MySid" href="/Index/Index">返回主页</Abar>
<div style="height:15px"></div>
<div>
    <Abar sid="@ViewBag.MySid" href="/Index/Index">精灵主页</Abar>&gt;图鉴    
</div>